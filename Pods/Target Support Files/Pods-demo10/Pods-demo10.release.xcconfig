FRAMEWORK_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/CleverTap-iOS-SDK"
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1 $(inherited) CLEVERTAP_HOST_WATCHOS
HEADER_SEARCH_PATHS = $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/CleverTap-iOS-SDK"
LD_RUNPATH_SEARCH_PATHS = $(inherited) '@executable_path/Frameworks' '@loader_path/Frameworks'
OTHER_CFLAGS = $(inherited)  -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/CleverTap-iOS-SDK"
OTHER_LDFLAGS = $(inherited) -ObjC -framework "CleverTapSDK.hostwatchos" -framework "CoreLocation" -framework "CoreTelephony" -framework "SystemConfiguration" -framework "UIKit" -framework "WatchConnectivity" -framework "WatchKit"
PODS_BUILD_DIR = $BUILD_DIR
PODS_CONFIGURATION_BUILD_DIR = $PODS_BUILD_DIR/$(CONFIGURATION)$(EFFECTIVE_PLATFORM_NAME)
PODS_ROOT = ${SRCROOT}/Pods
